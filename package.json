{
  "name": "downdog",
  "version": "0.1.17",
  "description": "sample application for yoga teachers",
  "private": true,
  "scripts": {
    "build": "npm run gulp build",
    "clean": "concurrently \"npm run gulp clean-dist\" \"npm run gulp clean-build\" ",
    "dist": "concurrently \"npm run dist:api\"  \"npm run dist:ng\"",
    "dist:api": "npm run gulp dist",
    "dist:ng": "ng build --prod",
    "e2e": "ng e2e",
    "gulp": "./node_modules/.bin/gulp",
    "lint": "ng lint",
    "ng": "ng",
    "start": "node ./dist/app-restify.js",
    "start:exp": "node ./dist/app-express.js",
    "start:dev": "concurrently \"npm run start:ng\" \"npm run start:api\" ",
    "start:ng": "ng serve --proxy-config src/client/proxy.conf.json",
    "start:api": "npm run gulp dev-restify",
    "start:api-exp": "npm run gulp dev-express",
    "test": "ng test",
    "test:api": "npm run gulp test",
    "ts:api": "./node_modules/.bin/ts-node src/app-restify.ts",
    "ts:api-exp": "./node_modules/.bin/ts-node src/app-express.ts",
    "watch": "npm run gulp dev-restify",
    "watch-exp": "npm run gulp dev-express"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/workofartyoga/downdog.git"
  },
  "author": "Arthur Anderson <arthur.anderson3@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/workofartyoga/downdog/issues"
  },
  "homepage": "https://github.com/workofartyoga/downdog#readme",
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/cli": "^1.1.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.6",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@types/restify": "^4.3.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "hammerjs": "^2.0.8",
    "human-readable-ids": "^1.0.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "pg": "^6.2.3",
    "restify": "^4.3.0",
    "rxjs": "^5.1.0",
    "sequelize": "^3.30.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@types/bluebird": "^3.5.0",
    "@types/body-parser": "^1.16.3",
    "@types/bunyan": "^0.0.37",
    "@types/dotenv": "^4.0.0",
    "@types/express": "^4.0.35",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.64",
    "@types/morgan": "^1.7.32",
    "@types/node": "~6.0.60",
    "@types/sequelize": "^4.0.55",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-jasmine": "^2.4.2",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-tslint": "^8.0.0",
    "gulp-typescript": "^3.1.6",
    "gulp-util": "^3.0.8",
    "jasmine": "^2.6.0",
    "jasmine-core": "~2.5.2",
    "jasmine-reporters": "^2.2.1",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  }
}
